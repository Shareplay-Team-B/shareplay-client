{
  "name": "Chrome Extension Webpack Template",
  "manifest_version": 3,
  "action": {
    "default_popup": "popup.html",
    "default_title": "Open"
  },
  "background": {
    "service_worker": "background.bundle.js"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*", "<all_urls>"],
      "js": ["content.bundle.js"]
    }
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  },
  "permissions": ["scripting", "proxy", "tts", "ttsEngine", "tabs", "webRequest", "webNavigation", "identity", "storage", "activeTab"],
  "permissions": ["scripting", "proxy", "tts", "ttsEngine", "tabs", "webRequest", "webNavigation", "identity", "activeTab"],
  "host_permissions": ["http://*/*", "https://*/*", "<all_urls>"],
  "oauth2": {
    "client_id": "414125495010-d6t3jc7j3b9f33f56iqc526jpo8v5412.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/userinfo.email"
    ]
  }
}
